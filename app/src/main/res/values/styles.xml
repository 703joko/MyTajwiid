<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
    </style>

    <!-- Fullscreen application theme. -->
    <style name="FullscreenTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="windowNoTitle">false</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>
    <style name="AppTheme.BrandedLaunch" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@drawable/splash_screen</item>
        <item name="colorPrimaryDark">#ff9e9e9e</item>

        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="TemaNun" parent="android:Theme.Holo.Light">
        <!-- Customize your theme here. -->
        <item name="windowNoTitle">true</item>
    </style>

</resources>
